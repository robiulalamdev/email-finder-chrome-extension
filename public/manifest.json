{
    "manifest_version": 3,
    "name": "Email Finder Pro",
    "version": "1.0",
    "description": "Scrape emails from the current page.",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "32": "icon.png"
    }
}